{
	"_version": "1.17.0",
	"sap.app": {
		"_version": "1.0.0",
		"id": "com.knauf.ca.lib",
		"type": "library",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"offline": true
	},
	"sap.ui": {
		"_version": "1.17.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.17.0",
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.71.52"
				},
				"sap.m": {
					"minVersion": "1.71.52"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}